#include <stdio.h>
int m;
int n;
int c[20][20];
char b[20][20];

void lcs_length(int x[], int y[])
{
    int i, j;

    for (i = 0; i <= m; i++)
    {
        c[i][0] = 0;
    }
    for (j = 0; j <= n; j++)
    {
        c[0][j] = 0;
    }
    for (i = 1; i <= m; i++)
    {
        for (j = 1; j <= n; j++)
        {
            if (x[i - 1] == y[j - 1])
            {
                c[i][j] = c[i - 1][j - 1] + 1;
                b[i - 1][j - 1] = 'D';
            }
            else if (c[i - 1][j] >= c[i][j - 1])
            {
                c[i][j] = c[i - 1][j];
                b[i - 1][j - 1] = 'U';
            }
            else
            {
                c[i][j] = c[i][j - 1];
                b[i - 1][j - 1] = 'L';
            }
        }
    }
    for (i = 0; i <= m; i++)
    {
        for (j = 0; j <= n; j++)
        {
            printf("%d ", c[i][j]);
        }
        printf("\n");
    }
    printf(“\n”);
    for (i = 0; i < m; i++)
    {
        for (j = 0; j < n; j++)
        {
            printf("%c ", b[i][j]);
        }
        printf("\n");
    }
}
void print_lcs(int x[], int i, int j)
{
    if (i == -1 || j == -1)
    {
        return;
    }
    if (b[i][j] == 'D')
    {
        print_lcs(x, i - 1, j - 1);
        printf("%d \t", x[i]);
    }
    else if (b[i][j] == 'U')
    {
        print_lcs(x, i - 1, j);
    }
    else
    {
        print_lcs(x, i, j - 1);
    }
}

int main()
{
    int sizex, sizey, i;
    int x[20], y[20];

    printf("\n Enter the size of first array \n");
    scanf("%d", &sizex);
    m = sizex;

    printf("\n Enter the value of first array \n");

    for (i = 0; i < sizex; i++)
    {
        scanf("%d", &x[i]);
    }

    printf("\n Enter the size of second array \n");
    scanf("%d", &sizey);
    n = sizey;
    printf("\n Enter the value of second array \n");

    for (i = 0; i < sizey; i++)
    {
        scanf("%d", &y[i]);
    }

    lcs_length(x, y);

    printf("\n LCS of given matrix is : \n");
    print_lcs(x, sizex - 1, sizey - 1);
}


Output : 
 Enter the size of first array 
8 
 Enter the value of first array 
1 0 0 1 0 1 0 1 
 
 Enter the size of second array 
11 
 Enter the value of second array 
0 1 0 1 1 0 1 1 1 1 0 
 
